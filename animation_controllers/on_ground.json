{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.on_ground": {
      "states": {
        "default": {
          "transitions": [
            {
              "on_ground": "q.is_on_ground"
            }
          ]
        },
        "on_ground": {
          "on_entry": [
            "/tag @s add ac:on_ground"
          ],
          "on_exit": [
            "/tag @s remove ac:on_ground"
          ],
          "transitions": [
            {
              "default": "!q.is_on_ground"
            }
          ]
        }
      }
    }
  }
}